{
    "name": "Rotsides/email-templates",
    "description": "Email Template Customizer",
    "type": "library",
    "keywords": [
        "rotsides",
        "email-templates"
    ],
    "homepage": "https://github.com/rotsides/email-templates",
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "spatie/phpunit-snapshot-assertions": "^4.2.0"
    },
    "autoload": {
        "psr-4": {
            "Rotsides\\EmailTemplates\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rotsides\\EmailTemplates\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rotsides\\EmailTemplates\\EmailServiceProvider"
            ]
        }
    }
}
